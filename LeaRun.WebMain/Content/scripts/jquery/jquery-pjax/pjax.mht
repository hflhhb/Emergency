  response.headers[<SPAN class=3D"pl-s"><SPAN =
class=3D"pl-pds">'</SPAN>X-PJAX-Version<SPAN =
class=3D"pl-pds">'</SPAN></SPAN>] <SPAN class=3D"pl-k">=3D</SPAN> <SPAN =
